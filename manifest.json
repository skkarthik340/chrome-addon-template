{
  "manifest_version": 2,
  "name": "TutorialPoint Extension",
  "version": "1.0.0.0",
  "icons": { "128":"icon128.png","16": "icon16.png"}, 
  "description": "Simple Extension",
   "background": {
      "scripts": ["background.js"]
    },
  "browser_action": {
                  "default_icon": "icon32.png" ,
           "default_title": "simple" ,
          "default_popup": "popup.html"
         },
"options_ui": {
    
    "page": "options.html"
  },
"permissions": [
"tabs",
      "contextMenus"
    ]

}
